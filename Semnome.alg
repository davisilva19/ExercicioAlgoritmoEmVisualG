algoritmo "contarate10"
// Função :
// Autor :
// Data : 11/4/2018
// Seção de Declarações 
var
  n2,cont,n1 : inteiro
inicio
      cont <- 1
      repita
      Escreva("Digite o ",cont,"o.numero ")
      Leia(n1)
      n2 <-n2+n1
      cont <- cont+1
      ate(cont >  10)
      Escreva ("A soma de todos os valores digitados e ",n2)
fimalgoritmo
